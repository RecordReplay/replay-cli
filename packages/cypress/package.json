{
  "name": "@replayio/cypress",
  "version": "1.7.20",
  "description": "Plugin to record your Cypress tests with Replay",
  "main": "./dist/src/index.js",
  "exports": {
    ".": "./dist/src/index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "if:exists": "node -e \"require('fs').existsSync(process.argv[1]) ? require('child_process').spawnSync(process.argv[2], process.argv.slice(3), {shell: true, stdio: 'inherit'}) : 0\"",
    "if:dist": "yarn run if:exists -- dist",
    "postinstall": "yarn run if:dist -- node ./dist/bin/replayio-cypress first-run",
    "prepare": "npm run build",
    "build": "rm -rf dist/ && tsc && chmod 755 dist/bin/*",
    "test": "echo \"Error: no test specified\"",
    "typecheck": "tsc --noEmit"
  },
  "bin": {
    "replayio-cypress": "./dist/bin/replayio-cypress.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/replayio/replay-cli.git"
  },
  "author": "",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/replayio/replay-cli/issues"
  },
  "homepage": "https://github.com/replayio/replay-cli/blob/main/packages/cypress/README.md",
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/node": "^20.11.27",
    "@types/semver": "^7.3.13",
    "@types/uuid": "^9.0.1",
    "@types/ws": "^8.5.10",
    "cypress": "^10.9.0",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@replayio/replay": "^0.21.4",
    "@replayio/test-utils": "^1.3.16",
    "chalk": "^4.1.2",
    "debug": "^4.3.4",
    "semver": "^7.5.2",
    "terminate": "^2.6.1",
    "txml": "^3.2.5",
    "uuid": "^8.3.2",
    "ws": "^8.14.2"
  },
  "peerDependencies": {
    "cypress": ">=5.3.0"
  }
}
