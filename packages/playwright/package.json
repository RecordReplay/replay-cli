{
  "name": "@replayio/playwright",
  "version": "1.1.16",
  "description": "Configuration utilities for using the Replay browsers with playwright",
  "main": "dist/src/index.js",
  "scripts": {
    "prepare": "yarn run build",
    "postinstall": "node ./dist/bin/replayio-playwright first-run",
    "build": "rm -rf dist/ && tsc && chmod 755 dist/bin/*",
    "test": "echo \"Error: no test specified\"",
    "typecheck": "tsc --noEmit"
  },
  "bin": {
    "replayio-playwright": "./dist/bin/replayio-playwright.js"
  },
  "devDependencies": {
    "@playwright/test": "1.19.x",
    "@types/node": "^20.11.27",
    "@types/uuid": "^8.3.4",
    "typescript": "^5.4.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/replayio/replay-cli.git"
  },
  "author": "",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/replayio/replay-cli/issues"
  },
  "homepage": "https://github.com/replayio/replay-cli/blob/main/packages/playwright/README.md",
  "dependencies": {
    "@replayio/replay": "^0.21.4",
    "@replayio/test-utils": "^1.3.16",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "@playwright/test": "1.19.x"
  }
}
